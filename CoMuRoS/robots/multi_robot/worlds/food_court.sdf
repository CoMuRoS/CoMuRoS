                    <geometry>
                        <plane>
                            <normal>0 0 1</normal>
                            <size>30 30</size>
                        </plane>
                    </geometry>
                <material>
                    <!-- very light pastel blue -->
                    <ambient>0.75 0.85 0.98 1</ambient>
                    <diffuse>0.75 0.85 0.98 1</diffuse>

                    <!-- keep specular very low -->
                    <specular>0.05 0.05 0.05 1</specular>
                </material>

                </visual>
            </link>
        </model>
            
